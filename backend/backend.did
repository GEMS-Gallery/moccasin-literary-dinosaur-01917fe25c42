type Time = int;
type Result_1 = 
 variant {
   err: text;
   ok;
 };
type Result = 
 variant {
   err: text;
   ok: File;
 };
type File = 
 record {
   content: blob;
   name: text;
   size: nat;
   uploadTime: Time;
 };
service : {
  deleteFile: (text) -> (Result_1);
  listFiles: () -> (vec File);
  uploadFile: (text, blob) -> (Result);
}
